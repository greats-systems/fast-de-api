{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fast-de-api/src",
  "monorepo": true,
  "root": "apps/fast-de-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/fast-de-api/tsconfig.app.json"
  },
  "projects": {
    "fast-de-api": {
      "type": "application",
      "root": "apps/fast-de-api",
      "entryFile": "main",
      "sourceRoot": "apps/fast-de-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fast-de-api/tsconfig.app.json"
      }
    },
    "comms": {
      "type": "application",
      "root": "apps/comms",
      "entryFile": "main",
      "sourceRoot": "apps/comms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comms/tsconfig.app.json"
      }
    }
  }
}